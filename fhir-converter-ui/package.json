{
  "name": "fhir-converter-ui",
  "version": "1.0.0",
  "description": "Web UI for Microsoft FHIR Converter",
  "scripts": {
    "install-deps": "npm run install-backend && npm run install-frontend",
    "install-backend": "cd backend && npm install",
    "install-frontend": "cd frontend && npm install --legacy-peer-deps",
    "build": "npm run build-backend && npm run build-frontend",
    "build-backend": "cd backend && npm run build",
    "build-frontend": "cd frontend && npm run build",
    "start": "concurrently \"npm run start-backend\" \"npm run start-frontend\"",
    "start-backend": "cd backend && npm run dev",
    "start-frontend": "cd frontend && npm start",
    "dev": "npm start",
    "test": "npm run test-backend && npm run test-frontend",
    "test-backend": "cd backend && npm test",
    "test-frontend": "cd frontend && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "fhir",
    "converter",
    "healthcare",
    "hl7",
    "ccda"
  ],
  "author": "FHIR Converter UI",
  "license": "MIT"
}
